@model TMSBilling.Models.Warehouse

<form id="warehouseForm">
    <input type="hidden" asp-for="ID" />

    <div class="mb-3 row">
        <label class="col-sm-3 col-form-label text-end">Warehouse ID</label>
        <div class="col-sm-8">
            <input asp-for="wh_code" class="form-control" />
        </div>
    </div>

    <div class="mb-3 row">
        <label class="col-sm-3 col-form-label text-end">Warehouse Name</label>
        <div class="col-sm-8">
            <input asp-for="wh_name" class="form-control" />
        </div>
    </div>

    <div class="text-end">
        <button type="submit" class="btn btn-sm btn-primary">Save</button>
    </div>
</form>
